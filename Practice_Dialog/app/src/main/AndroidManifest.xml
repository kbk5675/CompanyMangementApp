<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kbk.company_management">

    <application
        android:allowBackup="true"
        android:icon="@drawable/maintitleicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name="com.kbk.company_management.SettingActivity"></activity>
        <activity android:name="com.kbk.company_management.ItemsActivity" />
        <activity android:name="com.kbk.company_management.SalesActivity" />
        <activity android:name="com.kbk.company_management.ClientActivity" />
        <activity android:name="com.kbk.company_management.MenuActivity" />
        <activity android:name="com.kbk.company_management.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>